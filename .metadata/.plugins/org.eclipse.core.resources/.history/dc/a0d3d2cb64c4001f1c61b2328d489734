package ap.Pattern;

/*
 * "*****
 *  *****
 *  *****
 *  *****" 
 * */

public class SolidRectangle {
	public static void main(String[] args) {
		int c = 5;
		int r = 4;
		for(int i=1; i<=c; i++) {
			for(int j=1; j<=r; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
	}
	

}
